@model TestWebsite.Models.ZipLocationModel

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SubmitZipUrl</title>
</head>
<body>
    <div>
        @if (Model != null && Model.Directory != null)
        {
            if (Model.Exception != null)
            {
                <div>@Model.Exception</div>
            }
            <div>
                <a href="@Model.Directory">@Model.Directory</a>
            </div>
        }
    </div>
    <div>
        @using (Html.BeginForm())
        {
            @Html.TextBoxFor(x => x.ZipUrl)
            <input type="submit" />
        }
    </div>
</body>
</html>
